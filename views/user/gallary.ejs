<%-include("navbar.ejs")%>
<link rel="stylesheet" href="/user_assest/gallery.css">
<!-- AOS -->
<link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>


    <script>
        AOS.init({
            duration: 1000,
        });
    </script>
    <section >
        <div id="navbar"></div>
    </section>

    <section>
        <div class="container-fluid mb-md-5">
          <div class="row">
            <div class="col-12 p-0" data-aos="zoom-in">
              <div class="gallary-img d-flex justify-content-center align-items-center" style="background-image: url('/assest/img/photos/<%-gallary_background_page[0].gallary_background_image%>');">
                <h1>Gallery</h1>
              </div>
            </div>
          </div>
        </div>
      </section>

    <section>
        <div class="container py-md-5 p-0">
            <div class="row our-gallary">
                
                  
                 <% gallary_background_page.map((row,i)=>{ %>
                
                    <div class="col-md-3 mb-4 p-55" data-aos="zoom-in">
                        <div class="card h-100 ">
                            <img src="/assest/img/photos/<%-row.gallary_background_image%>" alt="" class="img-fluid rounded-3 p-3">
                        </div>
                    </div>  
                
                <% }) %>
                   
                  
                </div>
            </div>
        </div>
    </section>



    <section data-aos="zoom-in">
        <div id="footer"></div>
    </section>
</body>

</html>
<%-include("footer.ejs")%>